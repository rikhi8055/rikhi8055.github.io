---
import SocialModal from "src/components/modals/SocialModal.astro";
import CookieBanner from "src/components/other/CookieBanner.astro";
import Layout from "src/layouts/Layout.astro";

const title = "ThinkAiStudio.com | By Nitish Kumar Rikhi";
const description =
  "ThinkAiStudio: Expert UI/UX design services for startups and growing brands. From UX research and wireframing to prototyping, usability testing, and graphic design, we craft user-centered digital experiences that drive results and growth.";
---

<Layout title={title} description={description}>
  <!-- navigation -->
  <header
    id="mainNav"
    class="header shadow"
    itemscope
    itemtype="https://schema.org/WebSite"
  >
    <nav
      class="navbar mt-3 rounded-20 container navbar-expand-lg fixed-top p-2"
      aria-label="Main Navigation"
      itemscope
      itemtype="https://schema.org/SiteNavigationElement"
    >
      <div class="container">
        <a
          title="ThinkAiStudio.com"
          href="/"
          class="navbar-brand"
          itemprop="url"
        >
          <img
            width="40"
            alt="ThinkAiStudio logo - Expert UX/UI Design Studio"
            src="assets/img/template/logo.svg"
            itemprop="logo"
          />
        </a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarsExample09"
          aria-controls="navbarsExample09"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span> </span>
          <span> </span>
          <span> </span>
        </button>
        <div class="collapse navbar-collapse" id="navbarsExample09">
          <menu class="navbar-nav me-auto mt-0">
            <li>
              <a
                title="Services"
                class="nav-link"
                href="#services"
                itemprop="url">Services</a
              >
            </li>
            <li>
              <a
                title="Case Studies"
                class="nav-link"
                href="#recent-works"
                itemprop="url">Work</a
              >
            </li>
            <li>
              <a
                title="Testimonials"
                class="nav-link"
                href="#testimonials"
                itemprop="url">Testimonials</a
              >
            </li>
            <li>
              <a
                title="About the founder"
                class="nav-link"
                href="#about-me"
                itemprop="url">About</a
              >
            </li>
          </menu>
          <div class="d-md-flex d-sm-grid bd-highlight">
            <div
              class="head-available me-0 me-md-3 mb-3 mb-md-0 rounded-10 ps-1 pe-3 py-1 flex-fill bd-highlight d-none d-md-flex"
            >
              <div class="head-conteiner">
                <img
                  class="rounded-6 me-2"
                  alt="Nitish Rikhi - UX/UI Designer and Founder of ThinkAiStudio"
                  width="30"
                  src="assets/img/profile/avatar.webp"
                />
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="11"
                  height="11"
                  viewBox="0 0 11 11"
                  fill="none"
                >
                  <path
                    d="M5.5 10C7.98528 10 10 7.98528 10 5.5C10 3.01472 7.98528 1 5.5 1C3.01472 1 1 3.01472 1 5.5C1 7.98528 3.01472 10 5.5 10Z"
                    fill="#18B946"
                    stroke="#F0F0F0"
                    stroke-width="2"></path>
