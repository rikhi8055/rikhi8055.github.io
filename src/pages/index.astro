---
import SampleModal from "src/components/modals/SampleModal.astro";
import CookieBanner from "src/components/other/CookieBanner.astro";
import Layout from "src/layouts/Layout.astro";

const title = "ThinkAiStudio.com | By Nitish Kumar Rikhi";
const description =
  "ThinkAiStudio: Expert UI/UX design services for startups and growing brands. From UX research and wireframing to prototyping, usability testing, and graphic design, we craft user-centered digital experiences that drive results and growth.";
---

<Layout title={title} description={description}>
  <!-- navigation -->
  <header
    id="mainNav"
    class="header shadow"
    itemscope
    itemtype="https://schema.org/WebSite"
  >
    <nav
      class="navbar mt-3 rounded-20 container navbar-expand-lg fixed-top p-2"
      aria-label="Main Navigation"
      itemscope
      itemtype="https://schema.org/SiteNavigationElement"
    >
      <div class="container">
        <a
          title="main logo"
          href="index.html"
          class="navbar-brand"
          itemprop="url"
        >
          <img
            width="40"
            alt="UI Forge logo - Expert UX/UI Design Studio"
            src="assets/img/template/logo.svg"
            itemprop="logo"
          />
        </a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarsExample09"
          aria-controls="navbarsExample09"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span> </span>
          <span> </span>
          <span> </span>
        </button>
        <div class="collapse navbar-collapse" id="navbarsExample09">
          <menu class="navbar-nav me-auto mt-0">
            <li class="nav-item dropdown">
              <a
                class="nav-link dropdown-toggle"
                href="index.html#"
                id="navbarDropdown"
                role="button"
                data-bs-toggle="dropdown"
                aria-expanded="false"
              >
                Services
              </a>
              <ul class="dropdown-menu pb-0" aria-labelledby="navbarDropdown">
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/ux-ui-design-services.html"
                    itemprop="url">UX/UI design</a
                  >
                </li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/custom-web-design-development.html"
                    itemprop="url">Web design and development</a
                  >
                </li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/branding-and-visual-identity.html"
                    itemprop="url">Branding & identity</a
                  >
                </li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/mobile-app-design.html"
                    itemprop="url">Mobile design</a
                  >
                </li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/no-code-web-development.html"
                    itemprop="url">No code web development</a
                  >
                </li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/motion-design-animation.html"
                    itemprop="url">Motion design</a
                  >
                </li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/illustrations.html"
                    itemprop="url">Illustrations</a
                  >
                </li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services/iconography-icon-design.html"
                    itemprop="url">Icons</a
                  >
                </li>
                <li><hr class="dropdown-divider" /></li>
                <li>
                  <a
                    title="Services nav link"
                    class="dropdown-item"
                    href="https://www.uiforge.design/services.html"
                    itemprop="url">All services</a
                  >
                </li>
              </ul>
            </li>
            <li>
              <a
                title="Case Studies nav link"
                class="nav-link"
                href="index.html#case-studies"
                itemprop="url">Case Studies</a
              >
            </li>
            <li>
              <a
                title="Pricing nav link"
                class="nav-link"
                href="https://www.uiforge.design/pricing.html"
                itemprop="url">Pricing</a
              >
            </li>
            <li>
              <a
                title="About nav link"
                class="nav-link"
                href="https://www.uiforge.design/about.html"
                itemprop="url">About</a
              >
            </li>
          </menu>
          <div class="d-md-flex d-sm-grid bd-highlight">
